{
    "compilerOptions": {
        "target": "esnext",
        "module": "nodenext",
        "strict": true,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true,
        "outDir": "../../build/main",
        "allowJs": true,
        "noImplicitAny": false
    },
    "exclude": ["static"]
}
